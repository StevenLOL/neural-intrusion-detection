isbn = {1364-0321},
issn = {18790690},
journal = {Renewable and Sustainable Energy Reviews},
keywords = {Data Mining,Energy ManagementSystems,Knowledgediscoveryindatabases,Machine Learning,Microgrid planning,Sustainability},
pages = {615--630},
publisher = {Elsevier},
title = {{A knowledge discovery in databases approach for industrial microgrid planning}},
url = {http://dx.doi.org/10.1016/j.rser.2016.01.091},
volume = {60},
year = {2016}
