journal = {Journal of Ocean Engineering and Science},
keywords = {Artificial neural network,Coastal inundation,Remot,artificial neural network,coastal inundation,remote sensing,shoreline,soft computing,support vector machine},
number = {4},
pages = {247--255},
publisher = {Elsevier B.V.},
title = {{Application Of Soft Computing Techniques In Coastal Study – A Review}},
