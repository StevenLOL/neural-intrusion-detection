issn = {10848045},
journal = {Journal of Network and Computer Applications},
keywords = {ARIMA model,Adaptive Neuro Fuzzy Inference System (ANFIS),Damage prediction,Structural health monitoring,Wireless sensor and actuator networks,networks,structural health monitoring,wireless sensor and actuator},
number = {November 2016},
pages = {123--140},
