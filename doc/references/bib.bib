journal = {Procedia Computer Science},
keywords = {Cross layer analysis,Data Mining,Intrusion Detection in MANET,Intrusion classification,Self Learning IDS},
pages = {292--297},
publisher = {Elsevier Masson SAS},
title = {{Self Adaptive Intrusion Detection Technique Using Data Mining Concept in an Ad-hoc Network}},
